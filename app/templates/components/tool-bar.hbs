{{#paper-sidenav-container class="inner-sidenav"}}
{{#paper-sidenav
	class="md-whiteframe-z2"
	name="left"
	open=leftSideBarOpen
	lockedOpen = false
	onToggle=(action (mut leftSideBarOpen))}}
	{{#paper-toolbar as |toolbar|}}
		{{#paper-toolbar-tools}}Agile Hero{{/paper-toolbar-tools}}
	{{/paper-toolbar}}
		{{#link-to "dashboard"}}{{#paper-content}}Dashboard{{/paper-content}}{{/link-to}}
		{{#link-to "management"}}{{#paper-content}}User Management{{/paper-content}}{{/link-to}}
		{{#link-to "discuss"}}{{#paper-content}}Discuss{{/paper-content}}{{/link-to}}
		{{#link-to "discover-project"}}{{#paper-content}}Discover Projects{{/paper-content}}{{/link-to}}
	{{/paper-sidenav}}
	

{{#paper-toolbar}}
	  {{#paper-toolbar-tools}}
		    
				{{#paper-sidenav-toggle name="left" as |toggleAction|}}
					{{#paper-button classNames="hide-xl" onClick=(action toggleAction) iconButton=true}}
						{{paper-icon "menu"}}
					{{/paper-button}}
				{{/paper-sidenav-toggle}}
		    <h2>
		      Agiler Hero
		    </h2>
		    <span class="flex"></span>
		    {{#paper-button iconButton=true}}
		      {{paper-icon "search"}}
		    {{/paper-button}}
		    {{#paper-button iconButton=true}}
		      {{paper-icon "notification_important"}}
		    {{/paper-button}}
		    {{#paper-button iconButton=true}}
		      {{paper-icon "account_circle"}}
		    {{/paper-button}}
		    {{#paper-button iconButton=true}}
		      {{paper-icon "more_vert"}}
		    {{/paper-button}}
	  {{/paper-toolbar-tools}}
{{/paper-toolbar}}
{{/paper-sidenav-container}}

