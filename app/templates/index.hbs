<div class="container container-projects ">
    <center>
        
        <h2>Looking for Projects?</h2>
        {{paper-input label="Discover Projects" value=name onChange=(action (mut name))}}
       
        <div class="layout-row layout-xs-column flex">
            <div class="flex">
                <div id="google_btn">
                    
                    {{#paper-button raised=true warn=true onClick=(action "authenticateSession")}}
                        {{paper-icon "chevron_right"}}
                            Login With Google
                    {{/paper-button}}

                    {{!-- {{#if session.isAuthenticated}} --}}
                        <h1>Hey {{session.currentUser.name}}</h1>
                    {{!-- {{/if}} --}}
                    
                </div>
            </div>
            <div class="flex">
                <div id="github_btn">
                    {{#link-to "create-team.team-name"}}
                    {{#paper-button raised=true}}
                        {{paper-icon "chevron_right"}}
                            Login With Github
                    {{/paper-button}}
                    {{/link-to}}
                </div>
            </div>
        </div>
    </center>    
</div>
{{outlet}}